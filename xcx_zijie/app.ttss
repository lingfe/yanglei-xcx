  transition: all 300ms ease;
  display: flex;
  flex-direction: column;
  align-items: stretch;
  box-sizing: border-box;
  box-shadow: 2px 2px 5px #888888;
  padding-bottom: 20px;

}

.list-hover {
  opacity: 0.5;
}

.list-item {
  width: 100%;
